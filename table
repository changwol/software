CREATE TABLE MEMBER(
       MB_NUM INT AUTO_INCREMENT,
       MB_NAME VARCHAR(20) NOT NULL,
       MB_BDAY DATE NOT NULL,
       MB_GENDER VARCHAR(1) NOT NULL,
       MB_HOME VARCHAR(100),
       MG_NUM INT,
       LV_DATE VARCHAR(100),
       PRIMARY KEY(MB_NUM)
);
CREATE TABLE EMERGENCY(
       EMC_CALL INT(1),
       MB_HOME VARCHAR(100) NOT NULL,
       MEDICINE 0VARCHAR(100) DEFAULT NULL,
       CONSIDER VARCHAR(100) DEFAULT NULL
       );
 CREATE TABLE MANAGER(
       MG_NUM INT AUTO_INCREMENT,
       MG_NAME VARCHAR(20) NOT NULL,
       MG_PNUM VARCHAR(100) NOT NULL
       PRIMARY KEY(MG_NUM)       
);
CREATE TABLE MEDICINE(
       MB_NUM INT NOT NULL,
       MN_MEDICINE VARCHAR(50),
       MN_TIME TIME,
       AF_MEDICINE VARCHAR(50),
       AF_TIME TIME,
       EV_MEDICINE VARCHAR(50),
       EV_TIME TIME
) ENGINE MYISAM;
